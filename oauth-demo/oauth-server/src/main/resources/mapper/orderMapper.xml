<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.myoauth.oauthserver.mapper.OrderMapper" >


    <select id="selectOrderByOrderId" resultType="com.myoauth.oauthserver.domain.Order">
        SELECT
        id,product_name as productName,payment
        FROM
        t_order where id = #{id}
    </select>


</mapper>